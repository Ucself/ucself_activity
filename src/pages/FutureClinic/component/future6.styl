.secondPage_title {
    position: relative;
}

.secondPage_sequence {
    position: absolute;
    font-size: 160px;
    font-family: SourceHanSansCN-Heavy;
    font-weight: 800;
    color: rgba(248, 252, 251, 1);
    line-height: 160px;
    top: 0px;
    left: 21px;
    z-index: -1;
}

.secondPage_title_parent {
    display: flex;
    flex-direction: row;
    flex-wrap: nowrap;
    justify-content: flex-start;
    align-items: center;
}

.secondPage_title_tag {
    width: 9px;
    height: 53px;
    background: linear-gradient(
        98deg,
        rgba(57, 193, 160, 1) 0%,
        rgba(34, 166, 195, 1) 100%
    );
    margin-top: 56px;
    z-index: 2;
}

.secondPage_title_name {
    font-size: 56px;
    font-family: PingFangSC-Medium;
    font-weight: 500;
    color: rgba(78, 177, 51, 1);
    line-height: 56px;
    background: linear-gradient(
        56deg,
        rgba(57, 193, 160, 1) 0%,
        rgba(34, 166, 195, 1) 100%
    );
    -webkit-background-clip: text;
    -webkit-text-fill-color: transparent;
    margin-left: 31px;
    margin-top: 56px;
    z-index: 3;
}

.secondPage_subtitle {
    font-size: 24px;
    font-family: PingFangSC-Light;
    font-weight: 300;
    color: rgba(0, 0, 0, 0.5);
    line-height: 33px;
    margin-left: 40px;
    margin-top: 16px;
    text-align: left;
    z-index: 3;
}

.secondPage_detailOne {
    background: rgba(36, 158, 141, 0.03);
    border-radius: 4px;
    margin-top: 56px;
    margin-left: 30px;
    margin-right: 30px;
}

.secondPage_detailOne_text {
    font-size: 28px;
    font-family: PingFangSC-Light;
    font-weight: 300;
    color: rgba(51, 51, 51, 1);
    line-height: 48px;
    margin: 8px 24px 8px 10px;
}

.secondPage_detailTwoN {
    margin-top: 32px;
    margin-left: 30px;
    margin-right: 30px;
    /* width: calc(100% - 60px); */
}

.secondPage_detailTwo_image {
    width: 100%;
}

.secondPage_detailThreeN {
    margin-top: 72px;
    margin-left: 40px;
    margin-right: 40px;
    font-size: 28px;
    font-family: PingFangSC-Light;
    font-weight: 300;
    color: rgba(51, 51, 51, 1);
    line-height: 48px;
}